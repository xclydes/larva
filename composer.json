{
	"name" : "xclydes/larva",
	"description" : "A library to aid in the quickly developing CRUD functionality within a Larvel 5 applicaiton.",
	"require" : {
		"php" : ">=5.5.0",
		"view-components/eloquent-data-processing": "^1.2",
		"view-components/grids": "^0.5.9",
		"laravel/framework": "^5.0",
		"illuminate/support" : "5.*@dev",
		"laravelcollective/html" : "5.*",
		"kris/laravel-form-builder" : "^1.6",
		"doctrine/dbal" : "^2.5"
	},
	"license" : "MIT",
	"authors" : [{
			"name" : "Jermaine Rattray",
			"email" : "xclydes@gmail.com"
		}
	],
	"autoload" : {
		"psr-0" : {
			"Xclydes\\Larva" : "src/"
		}
	},
	"extra": {
		"laravel": {
			"providers": [
				"Xclydes\\Larva\\LarvaServiceProvider"
			]
		}
	},
	"minimum-stability" : "dev",
	"prefer-stable" : true
}